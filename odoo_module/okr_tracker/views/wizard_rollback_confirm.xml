<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="view_okr_rollback_confirm_form" model="ir.ui.view">
    <field name="name">okr.rollback.confirm.form</field>
    <field name="model">okr.rollback.confirm</field>
    <field name="arch" type="xml">
      <form string="Confirm Rollback">
        <group>
          <field name="migration_log_id"/>
        </group>
        <footer>
          <button string="Confirm Rollback" type="object" name="action_confirm" class="btn-danger"/>
          <button string="Cancel" class="btn-secondary" special="cancel"/>
        </footer>
      </form>
    </field>
  </record>
</odoo>
